{
  "name": "md-http-server",
  "version": "1.2.0",
  "description": "A simple HTTP server for rendering markdown files with Mermaid diagrams and Table of Contents",
  "main": "dist/index.js",
  "bin": {
    "md-http-server": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "markdown",
    "server",
    "http",
    "md",
    "renderer",
    "mermaid",
    "diagrams",
    "viewer",
    "toc",
    "table-of-contents",
    "navigation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "marked": "^11.1.0",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}